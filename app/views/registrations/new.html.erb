<h1>New Register</h1>
<%# <p>Find me in app/views/registrations/new.html.erb</p> %>

<%= form_with model: @user, url: sign_up_path do |f|  %>
    <%= f.label :email %>
    <%= f.text_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.label password_confirmation %>
    <%= f.password_field :password_confirmation %>

    <%= f.submit "Sign Up" %>
<% end %>